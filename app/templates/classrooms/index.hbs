{{ main-header }}
<div class="route-classrooms">
  <h1>Classrooms</h1>
  {{#link-to
    "classrooms.new"
  }}
    <button>Add new classroom</button>
  {{/link-to}}
  <ul>
    <table>
      <thead>
        <th>Start hour</th>
        <th>End hour</th>
      </thead>
      <tbody>
        {{#each model.reservations as |reservation|}}
          <tr>
            <td>{{reservation.label}}</td>
            <td>{{reservation.day}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </ul>
</div>