<div>
  <h1>Reservations</h1>

  <ul>
    <table>
      <thead>
      <th>day</th>
      <th>starthour</th>
      <th>endhour</th>
      <th>label</th>
      <th>studentgroupid</th>
      </thead>

      <tbody>
      {{#each model as |reservation|}}
        <tr>
          <td>{{reservation.day}}</td>
          <td>{{reservation.startHour}}</td>
          <td>{{reservation.endHour}}</td>
          <td>{{reservation.label}}</td>
          <td>{{reservation.studentgroup}}</td>
        </tr>
      {{/each}}
      </tbody>
    </table>
  </ul>
</div>